# comments begin with '#'
# you can test this database using svtest utility under build/

## section 1: global config
cfg_dotssh /home/user/.ssh
cfg_pubkeyis pk+
# ssh executable config (optional)
cfg_ssh ssh
cfg_scp scp
cfg_sshd /usr/sbin/sshd

## section 2: components
# selfname(client hostname for remote forwarding)
com_selfname1 localhost
com_selfname2 example.test
com_selfname3 127.0.0.1
# storage(src/dest pool for file transfer)
com_storage1 test
com_storage2 test2
# identity key (auth key or hostkey : private key only)
com_id1 id_rsa
com_id2 id_server
com_r_pubkey1 aADb4DAc0BEd4EFefA5BCD0EFA1Dpy7DAt8hBEonEF

## section X1: EUX config
# user's home directory
eux_home /home/user
# sve executable path
eux_bin .

## section 3: test entry
# remote entry / hybrid entry
test2 127.0.0.1 22 user
name_test2 2
id_test2 1
hkey_test2 1
fwd2loc_test2 6000::6010
fwd2rem_test2 6001::6000
stor_lc_test2 2
stor_rm_test2 2
pk+test2 aADb4DAc0BEd4EFefA5BCD0EFA1Dpy7DAt8hBEonEF
r_dotssh_test2 /home/user/.ssh
r_pubkey_test2 1

# server-only entry
server localhost 22 john_admin
listen_server 22
hkey_server 2
